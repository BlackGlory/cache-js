{
  "name": "@blackglory/cache-js",
  "version": "0.6.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": {
    "./lib/utils/rpc-client.js": "./lib/utils/rpc-client.browser.js"
  },
  "repository": "git@github.com:BlackGlory/cache-js.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "docker:test": "run-s docker:test:clean docker:test:build docker:test:run docker:test:clean",
    "docker:coverage": "run-s docker:test:clean docker:test:build docker:test:coverage docker:test:clean",
    "docker:test:build": "docker-compose --project-name cache-js --file docker-compose.test.yml build",
    "docker:test:run": "docker-compose --project-name cache-js --file docker-compose.test.yml run --rm test",
    "docker:test:clean": "docker-compose --project-name cache-js --file docker-compose.test.yml down",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build docker:test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "^29.2.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "husky": "4",
    "jest": "^29.3.1",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-patch": "^2.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.4.4"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.2.0",
    "@delight-rpc/extra-native-websocket": "^0.4.0",
    "@delight-rpc/extra-websocket": "^0.4.0",
    "delight-rpc": "^5.0.0",
    "extra-abort": "^0.2.0",
    "extra-native-websocket": "^0.3.1",
    "extra-websocket": "^0.3.0",
    "ws": "^8.11.0"
  }
}
